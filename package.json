{
  "name": "pev2",
  "keywords": [
    "PostgreSQL",
    "vuejs",
    "SQL",
    "plan"
  ],
  "version": "0.3.0",
  "main": "./dist/pev2.common.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "build": "vue-cli-service build --target lib --name pev2 ./src/components/Plan.vue",
    "build-app": "vue-cli-service build --dest dist-app",
    "test:unit": "jest"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/dragscroll": "0.0.0",
    "@types/lodash": "^4.14.141",
    "@types/moment-duration-format": "^2.2.2",
    "clarinet": "^0.12.3",
    "core-js": "^3.6.4",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "moment-duration-format": "^2.3.2",
    "splitpanes": "^2.2.1",
    "tippy.js": "^6.0.0",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-property-decorator": "^8.2.2",
    "vue-tippy": "^4.0.3"
  },
  "devDependencies": {
    "@types/jest": "^25.1.2",
    "@vue/cli-plugin-babel": "^4.0.5",
    "@vue/cli-plugin-typescript": "^4.0.5",
    "@vue/cli-service": "^4.0.5",
    "@vue/test-utils": "^1.0.0-beta.29",
    "axios": "^0.19.2",
    "babel-core": "^6.26.3",
    "husky": "^4.2.2",
    "jest": "^25.3.0",
    "lint-staged": "^10.0.7",
    "moment-locales-webpack-plugin": "^1.1.0",
    "node-sass": "^4.12.0",
    "prettier": "2.0.4",
    "sass-loader": "^8.0.0",
    "ts-jest": "^25.2.0",
    "typescript": "^3.6.3",
    "vue-jest": "^3.0.5",
    "vue-router": "^3.1.3",
    "vue-template-compiler": "^2.5.21"
  },
  "gitHooks": {
    "pre-commit": "lint-staged blah"
  },
  "lint-staged": {
    "*.ts": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost/",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  }
}
